    <body>
        <form action="/result" method="post">
            <p>Your Name: <input required type="text" name="name" value=""></p>
            <p>Dojo Location: <select required name="location">
                <option value="" disabled selected>Choose Location</option>
                <option value="Washington DC">Washington DC</option>
                <option value="Seattle">Seattle</option>
                <option value="San Jose">San Jose</option>
                <option value="New York City">New York City</option>
            </select></p>
            <p>Favorite Language: <select required name="language">
                <option value="" disabled selected>Choose a Lanuage</option>
                <option value="JavaScript">JavaScript</option>
                <option value="Python">Python</option>
                <option value="Ruby">Ruby</option>
                <option value="Swift">Swift</option>
            </select></p>
            <p>Comment (optional):</p>
            <textarea name="comment" rows="8" cols="80"></textarea>
            <input type="submit" name="" value="Submit!">
        </form>
    </body>
</html>
